create database Cinemus;
use cen4010s2020_g12
show tables;
create table User ( username char(20) NOT NULL,
password char(20) NOT NULL,
email char(320) NOT NULL,
currentroom char(20),
Primary Key (username),
Foreign Key (currentroom) REFERENCES Room (rid))
create table Room (rid char(20),
name char(20) NOT NULL,
settings char(20) NOT NULL,
currentvideo char(20),
numusers char(20),
director char(20),
Foreign Key (director) REFERENCES User(username))
create table Tag (tagname char(20),
Primary Key (tagname))
create table Usertag (uname char(20),
tname char(20),
Foreign Key(uname) REFERENCES User(username),
Foreign Key(tname) REFERENCES Tag(tagname))
create table Roomtag (rname char(20),
tname char(20),
Foreign Key(rname) REFERENCES Room(roomname),
Foreign Key(tname) REFERENCES Tag(tagname));
create table User ( username char(20) NOT NULL, password char(20) NOT NULL, email char(320) NOT NULL, currentroom char(20), Primary Key (username), Foreign Key (currentroom) REFERENCES Room (rid))  create table Room (rid char(20), name char(20) NOT NULL, settings char(20) NOT NULL, currentvideo char(20), numusers char(20), director char(20), Foreign Key (director) REFERENCES User(username))  create table Tag (tagname char(20), Primary Key (tagname))  create table Usertag (uname char(20), tname char(20), Foreign Key(uname) REFERENCES User(username), Foreign Key(tname) REFERENCES Tag(tagname))  create table Roomtag (rname char(20), tname char(20), Foreign Key(rname) REFERENCES Room(roomname), Foreign Key(tname) REFERENCES Tag(tagname));
