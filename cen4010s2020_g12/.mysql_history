use cen4010s2020_g12;
drop table room;
drop table Room;
drop table Usertag;
drop table Roomtag;
create table Room (rid int(20) AUTO_INCREMENT,
roomname char(20) NOT NULL,
settings char(20) NOT NULL,
currentvideo char(20),
numusers char(20),
director char(20) NOT NULL,
Foreign Key (director) REFERENCES User(username));
create table Room (rid int(20) AUTO_INCREMENT, roomname char(20) NOT NULL, settings char(20) NOT NULL, currentvideo char(20), numusers char(20), director char(20) NOT NULL, Foreign Key (director) REFERENCES User(username));
create table Room (rid int(20) AUTO_INCREMENT PRIMARY KEY,
roomname char(20) NOT NULL,
settings char(20) NOT NULL,
currentvideo char(20),
numusers char(20),
director char(20) NOT NULL,
Foreign Key (director) REFERENCES User(username));
create table Room (rid int(20) AUTO_INCREMENT PRIMARY KEY, roomname char(20) NOT NULL, settings char(20) NOT NULL, currentvideo char(20), numusers char(20), director char(20) NOT NULL, Foreign Key (director) REFERENCES User(username));
create table Usertag (uname char(20),
tname char(20),
Foreign Key(uname) REFERENCES User(username),
Foreign Key(tname) REFERENCES Tag(tagname));
create table Usertag (uname char(20), tname char(20), Foreign Key(uname) REFERENCES User(username), Foreign Key(tname) REFERENCES Tag(tagname));
create table Roomtag (rname char(20),
tname char(20),
Foreign Key(rname) REFERENCES Room(rid),
Foreign Key(tname) REFERENCES Tag(tagname));
create table Roomtag (rname char(20), tname char(20), Foreign Key(rname) REFERENCES Room(rid), Foreign Key(tname) REFERENCES Tag(tagname));
